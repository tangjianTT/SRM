<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.neusoft.srm.mapper.BaseMapper">

    <select id="findList" resultType="map">
        SELECT
          USER_CODE,
          USER_ACNT,
          USER_NAME
        FROM
        T_SYS_USER
        limit 0,1
    </select>
</mapper>